// ejercicio 13 del practico opcion 2.cpp : Defines the entry point for the console application.
// hacer un programa que imprema la tabla de sumar decimal (1 al 10), en cualquier formato.

#include "stdafx.h"
#include "conio.h"
#include "math.h"
#include <iostream>


using namespace std;

int main ()
{
	int i,n,s;
	s=2;
	n=10;
	cout<<"la tabla de sumar decimal es:\n";
	for(i=1;i<=n;i++)
	{
		s=1+i;
		cout<<"la suma es "<<1<<"+"<<i<<"="<<s;
	}
	getchar();
	getchar();

}
